<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Customer.
    -->
    <changeSet id="20210322191619-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="internal_user_id"
                                 baseTableName="customer"
                                 constraintName="fk_customer_internal_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="customer_shared_dinner"
                                 constraintName="fk_customer_shared_dinner_customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"/>

        <addForeignKeyConstraint baseColumnNames="shared_dinner_id"
                                 baseTableName="customer_shared_dinner"
                                 constraintName="fk_customer_shared_dinner_shared_dinner_id"
                                 referencedColumnNames="id"
                                 referencedTableName="shared_dinner"/>
        
    </changeSet>
</databaseChangeLog>
